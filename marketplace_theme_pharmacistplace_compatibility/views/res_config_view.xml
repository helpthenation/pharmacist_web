<odoo>
    <data>
        <record id="theme_pp_inherit_marketplace_base_config_settings_form_view" model="ir.ui.view">
            <field name="name">Marketplace.base.config.setting</field>
            <field name="model">marketplace.config.settings</field>
            <field name="inherit_id" ref="odoo_marketplace.marketplace_base_config_settings_form_view"/>
            <field name="arch" type="xml">
                <page name="config_website" position="inside">
                    <group>
                        <group name="pp_js_count" string="Pharmacies &amp; Distributor Count">
                            <label for="pharmacies_count"/>
                            <div>
                                <field name="pharmacies_count" class="oe_inline"/>
                            </div>
                            <label for="distributor_count"/>
                            <div>
                                <field name="distributor_count" class="oe_inline"/>
                            </div>
                        </group>
                    </group>
                </page>
            </field>
        </record>
    </data>
</odoo>