<odoo>
	<data>
		<function model="ir.values"
			id="default_allow_seller_to_create_profile_tabs"
			name="set_default"
			eval="('marketplace.config.settings', 'group_seller_profile_tabs', True)"
			/>

        <record id="profile_tab_data1" model="profile.tab">
            <field name="name">Video</field>
            <field name="active" eval="True"/>
        </record>

        <record id="profile_tab_data2" model="profile.tab">
            <field name="name">Gallery</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
