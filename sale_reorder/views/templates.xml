<odoo>

    <template id="wk_orders_reorder" name="Orders Reorder" inherit_id="website_sale.confirmation">
        <xpath expr="//div[@class='col-lg-8 col-sm-9']//div[@class='thanks_msg']" position="inside">
            <a id="wk_add_to_cart_reorder"
                class="btn btn-primary mt8 js_check_product a-submit pull-right mb32 "
